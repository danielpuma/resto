    <key>
      <column name="IdComprobante" />
    </key>
    <many-to-one name="Numerador" class="FastFood.Core.Numerador, FastFood.Core" fetch="select">
      <column name="IdNumerador" not-null="true" />
    </many-to-one>
  </joined-subclass>
</hibernate-mapping>