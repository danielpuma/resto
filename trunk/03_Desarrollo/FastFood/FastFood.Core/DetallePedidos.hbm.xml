<hibernate-mapping default-cascade="none" xmlns="urn:nhibernate-mapping-2.0">
  <class name="FastFood.Core.DetallePedidos, FastFood.Core" table="DetallePedidos">
    <id name="ID" type="System.Int32" column="IDDetallePedido" unsaved-value="0">
      <generator class="native" />
    </id>
    <property name="CodigoArticulo" type="System.String" column="CodigoArticulo" not-null="true" length="10" />
    <property name="DescripcionArticulo" type="System.String" column="DescripcionArticulo" not-null="true" length="255" />
    <property name="Cantidad" type="System.Decimal" column="Cantidad" not-null="true" />
    <property name="Precio" type="System.Decimal" column="Precio" not-null="true" />
    <property name="TotalLinea" type="System.Decimal" column="TotalLinea" not-null="true" />
    <property name="Pedido" type="System.Boolean" column="Pedido" not-null="true" />
    <property name="Tickeado" type="System.Boolean" column="Tickeado" not-null="true" />
    <property name="OrdenImpresion" type="System.Int32" column="OrdenImpresion" not-null="true" />
    <many-to-one name="MiArticulo" class="FastFood.Core.Articulo, FastFood.Core" fetch="join">
      <column name="IdArticulo" not-null="true" />
    </many-to-one>
    <many-to-one name="MiPedido" class="FastFood.Core.Pedido, FastFood.Core" fetch="join">
      <column name="IdPedido" not-null="true" />
    </many-to-one>
  </class>
</hibernate-mapping>